{
  "name": "@kaviar/nova",
  "version": "1.7.7",
  "description": "Nova is a relational query layer over MongoDB Node Drivers",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaviarjs/nova"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "test": "jest --verbose dist/__tests__/index.js",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --verbose --watch dist/__tests__/index.js",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov",
    "prepublishOnly": "npm run compile",
    "gen-doc": "typedoc ./src/index.ts --exclude ./src/__tests__ --out typeDocs --tsconfig tsconfig.json"
  },
  "dependencies": {
    "@deepkit/bson": "1.0.1-alpha.35",
    "@deepkit/type": "1.0.1-alpha.35",
    "@deepkit/crypto": "1.0.1-alpha.35",
    "@deepkit/core": "1.0.1-alpha.35",
    "dot-object": "^1.9.0",
    "graphql-fields": "^2.0.3",
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "mongodb": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.144",
    "@types/mongodb": "^3.6.18",
    "@types/node": "^14.17.4",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "jest": "^26.0.0",
    "jest-junit": "^10.0.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.5",
    "prettier": "^2.0.5",
    "sinon": "^7.5.0",
    "sinon-chai": "^2.9.0",
    "source-map-support": "^0.5.13",
    "typescript": "^4.0.2"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT"
}
