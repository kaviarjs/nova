{
  "name": "@kaviar/template",
  "version": "1.0.0",
  "description": "Kaviar Template",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaviarjs/template"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "start": "nodemon dist/index.js",
    "test": "jest --verbose dist/__tests__/index.js",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --verbose --watch dist/__tests__/index.js",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov",
    "prepublishOnly": "npm run compile",
    "gen-doc": "typedoc ./src/index.ts --exclude ./src/__tests__ --out typeDocs --tsconfig tsconfig.json"
  },
  "dependencies": {
    "@kaviar/nova": "^1.4.2",
    "benchmark": "^2.1.4",
    "knex": "^0.21.5",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.1",
    "mysql2": "^2.1.0",
    "pg": "^8.6.0",
    "pretty-hrtime": "^1.0.3",
    "rdtsc": "^2.0.0",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^25.2.1",
    "@types/mongodb": "^3.6.18",
    "@types/node": "^8.0.28",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "^26.0.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.13",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2",
    "unique-names-generator": "^4.5.0"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT"
}
